# Environment variables for Snowflake connection
# Copy this file to .env and update with your actual values

# Snowflake Account (format: ACCOUNT_IDENTIFIER or ORGNAME-ACCOUNT_NAME)
SNOWFLAKE_ACCOUNT=your_account_identifier

# Snowflake User
SNOWFLAKE_USER=your_username

# Authentication Method 1: Password (recommended for most users)
SNOWFLAKE_PASSWORD=your_password

# Authentication Method 2: SSO/External Browser (comment out password if using this)
# SNOWFLAKE_AUTHENTICATOR=externalbrowser

# Snowflake Role
SNOWFLAKE_ROLE=ACCOUNTADMIN

# Snowflake Warehouse
SNOWFLAKE_WAREHOUSE=COMPUTE_WH

# Snowflake Database
SNOWFLAKE_DATABASE=INTELLISTOCK_DB

# Snowflake Schema
SNOWFLAKE_SCHEMA=PUBLIC
